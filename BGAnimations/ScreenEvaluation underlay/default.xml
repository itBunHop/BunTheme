<ActorFrame><children>
	<Layer Type="ActorFrame" Name="panelP1" Condition="GAMESTATE:IsPlayerEnabled(0) "InitCommand="x,SCREEN_CENTER_X-310+DoublesOffset();y,SCREEN_CENTER_Y-66"><children>
		<Layer 
			File="pane.xml"
			Name="paneP1"
		/>
		
		<!-- label -->
		<Layer Type="ActorFrame" UpdateSpellDisplayP1MessageCommand="%function(self) local pn = 1 if oplot_hidden[pn] == 1 then self:hidden(0) else self:hidden(1) end end" InitCommand="x,100;y,20"><children>
			<Layer Type="BitmapText" Font="_nunito Bold small" Text="Fantastic" InitCommand="horizalign,right;shadowlength,0;zoom,0.7;y,20*0" OnCommand="@'diffuse,'..JudgmentColor(1)"/>
			<Layer Type="BitmapText" Font="_nunito Bold small" Text="Excellent" InitCommand="horizalign,right;shadowlength,0;zoom,0.7;y,20*1" OnCommand="@'diffuse,'..JudgmentColor(2)"/>
			<Layer Type="BitmapText" Font="_nunito Bold small" Text="Great" 	InitCommand="horizalign,right;shadowlength,0;zoom,0.7;y,20*2" OnCommand="@'diffuse,'..JudgmentColor(3)"/>
			<Layer Type="BitmapText" Font="_nunito Bold small" Text="Decent" 	InitCommand="horizalign,right;shadowlength,0;zoom,0.7;y,20*3" OnCommand="@'diffuse,'..JudgmentColor(4)"/>
			<Layer Type="BitmapText" Font="_nunito Bold small" Text="Way Off" 	InitCommand="horizalign,right;shadowlength,0;zoom,0.7;y,20*4" OnCommand="@'diffuse,'..JudgmentColor(5)"/>
			<Layer Type="BitmapText" Font="_nunito Bold small" Text="Miss" 		InitCommand="horizalign,right;shadowlength,0;zoom,0.7;y,20*5" OnCommand="@'diffuse,'..JudgmentColor(6)"/>
			<Layer Type="BitmapText" Font="common normal" Text="Mines" 			InitCommand="horizalign,right;shadowlength,0;zoom,0.7;y,20*6" />
			<Layer Type="BitmapText" Font="common normal" Text="Holds" 			InitCommand="horizalign,right;shadowlength,0;zoom,0.7;y,20*7" />
			<Layer Type="BitmapText" Font="common normal" Text="Rolls" 			InitCommand="horizalign,right;shadowlength,0;zoom,0.7;y,20*8" />
		</children></Layer>
		<!-- real stats -->
		<Layer Type="ActorFrame" InitCommand="x,110;y,20" UpdateSpellDisplayP1MessageCommand="%function(self) local pn = 1 if oplot_hidden[pn] == 1 then self:hidden(0) else self:hidden(1) end end" Name="StatsP1"><children>
			<Layer Type="BitmapText" Font="_nunito Bold small" Text="0" 		InitCommand="horizalign,left;shadowlength,0;zoom,0.7;y,20*0" OnCommand="%function(self) self:cmd('diffuse,'..JudgmentColor(1)) self:settext(STATSMAN:GetCurStageStats():GetPlayerStageStats(0):GetTapNoteScores(8)) end" />
			<Layer Type="BitmapText" Font="_nunito Bold small" Text="0" 		InitCommand="horizalign,left;shadowlength,0;zoom,0.7;y,20*1" OnCommand="%function(self) self:cmd('diffuse,'..JudgmentColor(2)) self:settext(STATSMAN:GetCurStageStats():GetPlayerStageStats(0):GetTapNoteScores(7)) end" />
			<Layer Type="BitmapText" Font="_nunito Bold small" Text="0" 		InitCommand="horizalign,left;shadowlength,0;zoom,0.7;y,20*2" OnCommand="%function(self) self:cmd('diffuse,'..JudgmentColor(3)) self:settext(STATSMAN:GetCurStageStats():GetPlayerStageStats(0):GetTapNoteScores(6)) end" />
			<Layer Type="BitmapText" Font="_nunito Bold small" Text="0" 		InitCommand="horizalign,left;shadowlength,0;zoom,0.7;y,20*3" OnCommand="%function(self) self:cmd('diffuse,'..JudgmentColor(4)) self:settext(STATSMAN:GetCurStageStats():GetPlayerStageStats(0):GetTapNoteScores(5)) end" />
			<Layer Type="BitmapText" Font="_nunito Bold small" Text="0" 		InitCommand="horizalign,left;shadowlength,0;zoom,0.7;y,20*4" OnCommand="%function(self) self:cmd('diffuse,'..JudgmentColor(5)) self:settext(STATSMAN:GetCurStageStats():GetPlayerStageStats(0):GetTapNoteScores(4)) end" />
			<Layer Type="BitmapText" Font="_nunito Bold small" Text="0" 		InitCommand="horizalign,left;shadowlength,0;zoom,0.7;y,20*5" OnCommand="%function(self) self:cmd('diffuse,'..JudgmentColor(6)) self:settext(STATSMAN:GetCurStageStats():GetPlayerStageStats(0):GetTapNoteScores(3)) end" />
			<Layer Type="BitmapText" Font="_nunito Bold small" Text="0" 		InitCommand="horizalign,left;shadowlength,0;zoom,0.7;y,20*6" OnCommand="%function(self) self:settext(STATSMAN:GetCurStageStats():GetPlayerStageStats(0):GetTapNoteScores(1)) end" />
			<Layer Type="BitmapText" 
				Font="_nunito Bold small" 
				Text="" 		
				InitCommand="horizalign,left;shadowlength,0;zoom,0.7;y,20*7"
				OnCommand="queuecommand,Set"
				SetCommand="%function(self) 
					local a = SCREENMAN:GetTopScreen():GetChild('HoldsTextP1'):GetText()
					
					self:settext(string.gsub(string.gsub(a,' ',''),'/',' / '))
				end" 
			/>
			<Layer Type="BitmapText" 
				Font="_nunito Bold small" 
				Text="" 		
				InitCommand="horizalign,left;shadowlength,0;zoom,0.7;y,20*8"
				OnCommand="queuecommand,Set"
				SetCommand="%function(self) 
					local a = SCREENMAN:GetTopScreen():GetChild('RollsTextP1'):GetText()
					
					self:settext(string.gsub(string.gsub(a,' ',''),'/',' / '))
				end" 
			/>
		</children></Layer>
		
		<!-- p1 score -->
		<Layer
			Type="BitmapText"
			Font="_inconsolata"
			InitCommand="shadowlength,0;x,290;y,40;zoom,0.7;horizalign,right"
			OnCommand="queuecommand,Set"
			SetCommand="%function(self) self:settext(SCREENMAN:GetTopScreen():GetChild('PercentP1'):GetChild('PercentP1'):GetText()) end"
		/>
		
		<!-- p1 mods -->
		<Layer
			Type="BitmapText"
			Font="_nunito small"
			InitCommand="shadowlength,0;x,8;y,270;zoom,0.45;horizalign,left;wrapwidthpixels,650"
			OnCommand="queuecommand,Set"
			SetCommand="%function(self) self:settext(SCREENMAN:GetTopScreen():GetChild('PlayerOptionsP1'):GetText()) end"
		/>
		
		<!-- p1 difficulty and description lol -->
		<Layer
			Type="BitmapText"
			Font="_nunito Bold small"
			InitCommand="shadowlength,0;x,4;y,-14;zoom,0.6;horizalign,left;maxwidth,500;queuecommand,Set"
			SetCommand="%function(self)
				if not GAMESTATE:IsCourseMode() then
					local a = GAMESTATE:GetCurrentSteps(0)
					local b = a:GetRadarValues()
					self:settext(DifficultyToThemedString(a:GetDifficulty())..' '..a:GetMeter())
				else
					local a = SCREENMAN('DifficultyMeterP1')
					
					self:settext(a(1):GetText()..' '..a(2):GetText())
				end
			end"
		/>
		
		<Layer
			Type="BitmapText"
			Font="_nunito small"
			InitCommand="shadowlength,0;x,4;y,2;zoom,0.5;horizalign,left;maxwidth,600;queuecommand,Set"
			SetCommand="%function(self)
				if not GAMESTATE:IsCourseMode() then
					local steps = GAMESTATE:GetCurrentSteps(0)
					self:settext(steps:GetDescription())
				else
					self:settext(ThemeHelpers.CourseStages..' file(s)')
				end
			end"
		/>
	</children></Layer>
	
