<ActorFrame
	Sound="Screen cancel/cancel.ogg"
> <children>
	<Layer
		Type="Quad"
		InitCommand="stretchto,SCREEN_CENTER_X-320,0,SCREEN_CENTER_X+320,SCREEN_HEIGHT;diffuse,#000000;diffusealpha,0"
		OffCommand="linear,0.5;diffusealpha,0.5"
	/>
	<Actor
		Type="Quad"
		InitCommand="hidden,1"
		StartTransitioningCommand="%function(self) SCREENMAN:GetTopScreen():playcommand('Off') end"
	/>
</children> </ActorFrame>

